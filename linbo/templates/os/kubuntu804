[OS]                       # Beginn einer Betriebssystemdefinition
Name = Kubuntu 8.04 Part$PART  # Name des Betriebssystems
Version =                  # Version (optional, frei waehlbar)
Description = Kubuntu 8.04 Part$PART          # Beschreibung
Image = $RECHNERGRUPPE-ku804-part$PART.rsync  # Dateiname des differentiellen Images
                                                    # (Erweiterung .rsync)
BaseImage = $RECHNERGRUPPE-ku804-part$PART.cloop    # Dateiname des Basisimages
                                                    # (Erweiterung .cloop)
Boot = /dev/$DEVICE$PART   # Partition, die Kernel & Initrd enthaelt
Root = /dev/$DEVICE$PART   # Partition, in die das BS installiert ist
Kernel = vmlinuz           # 
Initrd = initrd.img        # Relativer Pfad zur Initrd
Append = ro splash vga=791 root=/dev/$DEVICE$PART  # Kernel-Append-Parameter
StartEnabled = yes         # "Start"-Button anzeigen
SyncEnabled = yes          # "Sync"-Button anzeigen
RemoteSyncEnabled = yes    # Synchronisation mit dem Server zulassen
Hidden = yes               # blendet den OS-Reiter f√ºr dieses System
                           # aus
Autostart = no             # yes: startet dieses System ohne Benutzer-
                           # interaktion automatisch synchronisiert

